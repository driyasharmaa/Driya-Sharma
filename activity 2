name = "Orlaith"
age = 0
if name == "Alex" or name == "John" and age >= 2 :
    print ("Hello welcome!")
else :
    print ("Goodbye!")

print("Enter a number (numerator): ")
numn = int(input())
print ("Enter a number (denominator): ") 
numd = int(input())
if numn%numd == 0:
    print("\n" + str (numn)+ " is divisible by " + str(numd))
else :
        print("\n" + str (numn)+ " is not divisible by "+ str(numd))